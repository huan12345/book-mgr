               <a-button size=\"small\" type=\"primary\" @click=\"showUpdateModal = true\">编辑</a-button>\r\n                    &nbsp;\r\n                    <a-button size=\"small\" type=\"danger\" @click=\"remove\">删除</a-button>\r\n                </div>\r\n            </space-between>\r\n            <!-- 分割线 -->\r\n            <a-divider />\r\n            <!-- 书籍基础信息 -->\r\n            <div class=\"base-info\">\r\n                <!-- 第一行 -->\r\n                <div class=\"items\">\r\n                    <div class=\"item\">\r\n                        <div class=\"title\">价格</div>\r\n                        <div class=\"content\">{{ d.price }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <div class=\"title\">作者</div>\r\n                        <div class=\"content\">{{ d.author }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <div class=\"title\">分类</div>\r\n                        <div class=\"content\">{{ d.classify }}</div>\r\n                    </div>\r\n                </div>\r\n                <!-- 第二行 -->\r\n                <div class=\"items\">\r\n                    <div class=\"item\">\r\n                        <div class=\"title\">出版日期</div>\r\n                        <div class=\"content\">{{ formatTimestamp(d.publishDate) }}</div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </a-card>\r\n        \r\n        <div class=\"log\">\r\n            <!-- 出入库日志 -->\r\n            <a-card title=\"出入库日志\">\r\n                <template #extra>\r\n                    <span>\r\n                        <a href=\"javascript:;\" @click=\"logFilter('IN_COUNT')\">\r\n                            <CheckOutlined v-if=\"curLogType === 'IN_COUNT'\"/>\r\n                            入库日志\r\n                        </a>\r\n                    </span>\r\n                    <span style=\"margin-left: 12px;\">\r\n                        <a href=\"javascript:;\" @click=\"logFilter('OUT_COUNT')\">\r\n                            <CheckOutlined  v-if=\"curLogType === 'OUT_COUNT'\"/>\r\n                            出库日志\r\n                        </a>\r\n                    </span>\r\n                </template>\r\n                <div>\r\n                    <!-- 出入库日志列表 -->\r\n                    <a-table \r\n                        :data-source=\"log\" \r\n                        :columns=\"columns\" \r\n                        bordered \r\n                        :pagination=\"false\" \r\n                    >\r\n                        <!-- 时间 -->\r\n                        <template #createdAt=\"{ record }\">\r\n                            {{ formatTimestamp(record.meta.createdAt) }}\r\n                        </template>\r\n                    </a-table>\r\n                </div>\r\n\r\n                <space-between style=\"margin-top: 24px;\">\r\n                    <div />\r\n                    <a-pagination \r\n                        v-model:current=\"logCurPage\" \r\n                        :total=\"logTotal\"\r\n                        :page-size=\"10\" \r\n                        @change=\"setLogPage\"\r\n                    />\r\n                </space-between>\r\n            </a-card>\r\n        </div>\r\n        <update \r\n            v-model:show=\"showUpdateModal\"\r\n            :book=\"d\"\r\n            @update=\"update\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script src=\"./index.js\"></script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    @import './index.scss';\r\n</style>"]}]}