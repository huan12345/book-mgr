G,SAARA,KAAQ,CAACC,GAAD,EAAS;AAC1B,SAAOC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAP;AACH,CAFM,C,CAGP;;AACA,OAAO,IAAMI,eAAe,GAAG,SAAlBA,eAAkB,CAACC,EAAD,EAAQ;AACnC,MAAMC,IAAI,GAAG,IAAIC,IAAJ,CAASC,MAAM,CAACH,EAAD,CAAf,CAAb,CADmC,CACA;;AAEnC,MAAMI,IAAI,GAAGH,IAAI,CAACI,WAAL,EAAb,CAHmC,CAGH;;AAChC,MAAMC,EAAE,GAAGL,IAAI,CAACM,QAAL,KAAkB,CAA7B,CAJmC,CAIJ;;AAC/B,MAAMC,EAAE,GAAGP,IAAI,CAACQ,OAAL,EAAX,CALmC,CAKT;;AAE1B,MAAMC,EAAE,GAAGT,IAAI,CAACU,QAAL,EAAX,CAPmC,CAOR;;AAC3B,MAAMC,EAAE,GAAGX,IAAI,CAACY,UAAL,EAAX,CARmC,CAQN;;AAC7B,MAAMC,EAAE,GAAGb,IAAI,CAACc,UAAL,EAAX,CATmC,CASN;;AAE7B,mBAAUX,IAAV,cAAkBE,EAAlB,cAAwBE,EAAxB,cAA8BE,EAA9B,SAAmCE,EAAnC,cAAyCE,EAAzC;AACH,CAZM","sourcesContent":["import { message } from 'ant-design-vue';//提示框\r\nimport { formatCountdown } from 'ant-design-vue/lib/statistic/utils';\r\n// 处理请求数据并拿到结果\r\nexport const result = (response,authShowErrorMsg = true) => {\r\n    const { data } = response;// 请求数据\r\n    // code = 0，请求失败时，提示框提示\r\n    if ((data.code === 0) && authShowErrorMsg) {\r\n        message.error(data.msg);\r\n    }\r\n\r\n    return {\r\n        // 成功时做的事情\r\n        success(cb) {\r\n            if (data.code !== 0) {\r\n                cb(data, response);\r\n            }\r\n            return this;// 整个对象返回\r\n        },\r\n        // 失败时做的事情\r\n        fail(cb) {\r\n            if (data.code === 0) {\r\n                cb(data, response);\r\n            }\r\n            return this;\r\n        },\r\n        // 默认都会做的事情\r\n        finally(cb) {\r\n            cb(data, response);\r\n            return this;\r\n        },\r\n    };\r\n};\r\n// 复制\r\nexport const clone = (obj) => {\r\n    return JSON.parse(JSON.stringify(obj));\r\n};\r\n// 格式化时间戳ts\r\nexport const formatTimestamp = (ts) => {\r\n    const date = new Date(Number(ts)); // Number(ts)字符串转数字\r\n\r\n    const YYYY = date.getFullYear();// 年\r\n    const MM = date.getMonth() + 1;// 月\r\n    const DD = date.getDate();// 日\r\n\r\n    const hh = date.getHours();// 时\r\n    const mm = date.getMinutes();// 分\r\n    const ss = date.getSeconds();// 秒\r\n\r\n    return `${YYYY}/${MM}/${DD} ${hh}${mm}:${ss}` ;\r\n};\r\n  "]}]}