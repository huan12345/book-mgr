            />\r\n                <!-- 返回查询首页 if 显示/关闭-->\r\n                <a v-if=\"isSearch\" href=\"javascript:;\" @click=\"backAll\">返回</a>\r\n                </div>  \r\n                <!-- 添加书籍按钮 -->\r\n                <a-button @click=\"show = true\">添加一条</a-button>   \r\n            </space-between>\r\n\r\n            <a-divider />\r\n            <!-- 书籍表格信息 -->\r\n            <a-table \r\n                :columns=\"columns\" \r\n                :data-source=\"list\"\r\n                :pagination=\"false\"\r\n                bordered\r\n            >\r\n                <!-- 出版时间格式化 -->\r\n                <template #publishDate=\"data\">\r\n                    {{ formatTimestamp(data.record.publishDate) }}\r\n                </template>\r\n                <!-- 库存 -->\r\n                <template #count=\"data\">\r\n                    <a href=\"javascript:;\" @click=\"updateCount('IN_COUNT',data.record)\">入库</a>\r\n                    {{ data.record.count }}\r\n                    <a href=\"javascript:;\" @click=\"updateCount('OUT_COUNT',data.record)\">出库</a>\r\n                </template>\r\n                <!-- 删除书籍 -->\r\n                <template #actions=\"record\">\r\n                    <a href=\"javascript:;\" @click=\"toDetail(record)\">详情</a>\r\n                    &nbsp;\r\n                    <a href=\"javascript:;\" @click=\"update(record)\">编辑</a>\r\n                    &nbsp;\r\n                    <a href=\"javascript:;\" @click=\"remove(record)\">删除</a>\r\n                </template>\r\n            </a-table>\r\n            <!-- 分页组件 -->\r\n            <space-between style=\"margin-top: 23px\">\r\n                <div />\r\n                <!-- 分页组件 -->\r\n                <a-pagination \r\n                    v-model:current=\"curPage\" \r\n                    :total=\"total\"\r\n                    :page-size=\"10\" \r\n                    @change=\"setPage\"\r\n                />\r\n            </space-between>\r\n        </a-card>\r\n        \r\n        <!-- 添加一本书籍 -->\r\n        <add-one \r\n            v-model:show=\"show\"\r\n        />\r\n        <!-- 编辑书籍 -->\r\n        <update \r\n            v-model:show=\"showUpdateModal\"\r\n            :book=\"curEditBook\"\r\n            @update=\"updateCurBook\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script src=\"./index.jsx\"></script>\r\n\r\n<style lang=\"scss\" scoped>// scoped 防止全局样式污染\r\n    @import './index.scss';\r\n</style>"],"sourceRoot":""}]}