=\"curPage\" \r\n                    :total=\"total\"\r\n                    :page-size=\"10\" \r\n                    @change=\"setPage\"\r\n                ></a-pagination>\r\n            </flex-end>\r\n        </a-card>\r\n\r\n        <add-one\r\n            v-model:show=\"showAddModal\"\r\n            @getList=\"getUser\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script src=\"./index.js\"></script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    @import './index.scss';\r\n</style>\r\n"],"sourceRoot":""}]}